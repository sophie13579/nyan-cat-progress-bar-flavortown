{
  "manifest_version": 3,
  "name": "Nyan cat progress bar",
  "version": "1.0",
  "description": "Adds a Nyan cat progress bar for flavortown!",
  "icons": {
    "16": "images/icons/nyan-16.png",
    "32": "images/icons/nyan-32.png",
    "48": "images/icons/nyan-48.png",
    "128": "images/icons/nyan-128.png"
  },
  "content_scripts": [
    {
      "css": ["scripts/hide-progress.css"],
      "js": ["scripts/content.js"],
      "matches": [
        "https://flavortown.hackclub.com/kitchen",
        "https://flavortown.hackclub.com/my/achievements"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
  {
    "resources": [
      "images/nyan_cat/rainbow.png",
      "images/nyan_cat/nyan_cat.gif"
    ],
    "matches": [
      "https://flavortown.hackclub.com/*"
    ]
  }
]

}